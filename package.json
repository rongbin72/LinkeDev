{
  "name": "linkedev",
  "version": "1.0.0",
  "description": "Social Network",
  "main": "server",
  "scripts": {
    "build": "tsc -p .",
    "server": "ts-node-dev --respawn --transpileOnly --no-notify -- server.ts",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run gentype --prefix client\"",
    "start": "node build/server.js",
    "heroku-postbuild": "npm install --prefix client && npm run build --prefix client && npm run build"
  },
  "author": "Bin Rong",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^2.5.1",
    "axios": "^0.18.0",
    "bcryptjs": "^2.4.3",
    "config": "^3.1.0",
    "express": "^4.17.1",
    "express-validator": "^5.3.1",
    "graphql": "^14.3.1",
    "graphql-import": "^0.7.1",
    "gravatar": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.5.11"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "0.0.34",
    "@types/express": "^4.16.1",
    "@types/graphql": "^14.2.0",
    "@types/gravatar": "^1.8.0",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/mongoose": "^5.5.3",
    "@types/node": "^11.13.13",
    "concurrently": "^4.1.0",
    "ts-node-dev": "^1.0.0-pre.39",
    "typescript": "^3.5.1"
  }
}
